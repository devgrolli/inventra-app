{
  "expo": {
    "name": "stock-app",
    "slug": "stock-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/building.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/images/logo-w-name.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./src/assets/building.png"
    }
  }
}
